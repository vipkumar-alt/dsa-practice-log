class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int minVal=prices[0];
        int max_diff=0;

        for(int i=1;i<prices.size();i++){
            minVal=min(minVal,prices[i]);
            max_diff=max(max_diff,prices[i]-minVal);
        }

        return max_diff;
    }
};
